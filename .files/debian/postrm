set -e

. /etc/default/openmediavault
. /usr/share/openmediavault/scripts/helper-functions

if [ "$1" = "purge" ]; then

	FOLDER=""
	FILES=""
	for FILE in ${FILES}; do
		[ -f "${FOLDER}/${FILE}" ] && /bin/rm "${FOLDER}/${FILE}"
	done

fi

#DEBHELPER#

exit 0
